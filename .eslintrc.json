{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
    	"ecmaVersion": 6,
    	"ecmaFeatures": {
      		"jsx": true,
          "module": true,
          "experimentalObjectRestSpread": true
    	}
  	},
  	"plugins": [
    	"react"
  	],
  	"rules": {
    	"semi": "warn",
      "no-console": "warn"
  	},
  	"env": {
    	"browser": true,
    	"node": true,
      "es6": true
  	}
}